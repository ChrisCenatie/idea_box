<p>
  <% flash.each do |key, message| %>
    <%= content_tag :div, message, class: "flash_#{key}" %>
  <% end %>
</p>

<%= form_for(@user) do |f| %>
  <%= f.label :username %>
  <%= f.text_field :username %><br/>

  <%= f.label :password %><br/>
  <%= f.password_field :password %><br/>

  <%= f.submit "Create Account" %>
<% end %>
